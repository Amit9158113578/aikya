  <route id="PBQUpdateReqRoute">
    <from uri="activemq:queue:pbqupdatereqQ" id="pbqupdatereqQ">
      <description/>
    </from>
    <bean ref="PBQuoteListener" method="onMessage" id="PBQuoteUpdateReqBean"/>
    <log message="PBQ update Request conversion received  ${body}"/>
        <process ref="PBQuoteUpdateRequestProcessor" id="PBQuoteUpdateRequestProcessor"/>
        <log message="CarQuote service Request received for ProfessionalDocument : ${headers}" />
	        	 <log message="CarQuote service Request received for ProfessionalDocument :${body}" />
	        		<process ref="PBQuoteUpdateReqConfiguration" id="PBQuoteUpdateReqConfiguration"/>
	        	<log message="CarQuote service Request received for Response :${body}" />	
	  </route>
    </camelContext>
</blueprint>
